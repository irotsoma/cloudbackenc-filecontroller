{{#layout}}{{#title}}{{pageTitle}}{{/title}}
<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>{{extensionNameLabel}}</th>
                <th>{{extensionVersionLabel}}</th>
                <th>{{extensionUuidLabel}}</th>
            </tr>
        </thead>
        <tbody>
            {{#extensions}}
                <tr>
                    <td>{{extensionName}}</td>
                    <td>{{releaseVersion}}</td>
                    <td>{{extensionUuid}}</td>
                </tr>
            {{/extensions}}
        </tbody>
    </table>
</div>



{{/layout}}